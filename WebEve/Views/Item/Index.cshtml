@model IEnumerable<WebEve.Models.Item>

@{
    ViewBag.Title = "Index";
}
<script src="@Url.Content("~/Scripts/ItemAutoComplete.js")" type="text/javascript"></script>
<h2>Index</h2>

<p>
    <label> Item : </label>
    <input id="ItemSearch" />
    <input type='button' onclick='AddItem()' value='Add'/>
    <input type='hidden' id='itemId' />
</p>
<table>
    <thead>
        <tr>
            <th>
                ApiId
            </th>
            <th>
                Name
            </th>
            <th></th>
        </tr>
    </thead>
<tbody id="ItemList">
    @foreach (var item in Model)
    {
        { Html.RenderPartial("ItemTableTemplate", item); }
    }
</tbody>


</table>
